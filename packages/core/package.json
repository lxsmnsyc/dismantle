{
  "name": "dismantle",
  "version": "0.6.1",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "license": "MIT",
  "keywords": [
    "pridepack"
  ],
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "^25.0.8",
    "pridepack": "2.6.4",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "dependencies": {
    "@babel/core": "^7.28.6",
    "@babel/generator": "^7.28.6",
    "@babel/traverse": "^7.28.6",
    "@babel/types": "^7.28.6"
  },
  "scripts": {
    "prepublishOnly": "pridepack clean && pridepack build",
    "build": "pridepack build",
    "type-check": "pridepack check",
    "lint": "pridepack lint",
    "clean": "pridepack clean",
    "watch": "pridepack watch",
    "start": "pridepack start",
    "dev": "pridepack dev",
    "test": "vitest"
  },
  "description": "Universal \"use-server\" functions",
  "repository": {
    "url": "https://github.com/lxsmnsyc/dismantle.git",
    "type": "git"
  },
  "homepage": "https://github.com/lxsmnsyc/dismantle/tree/main/packages/core",
  "bugs": {
    "url": "https://github.com/lxsmnsyc/dismantle/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Alexis Munsayac",
  "private": false,
  "typesVersions": {
    "*": {
      "runtime": [
        "./dist/types/runtime/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "development": {
        "require": "./dist/cjs/development/index.cjs",
        "import": "./dist/esm/development/index.mjs"
      },
      "require": "./dist/cjs/production/index.cjs",
      "import": "./dist/esm/production/index.mjs"
    },
    "./runtime": {
      "types": "./dist/types/runtime/index.d.ts",
      "development": {
        "require": "./dist/cjs/development/runtime.cjs",
        "import": "./dist/esm/development/runtime.mjs"
      },
      "require": "./dist/cjs/production/runtime.cjs",
      "import": "./dist/esm/production/runtime.mjs"
    }
  },
  "types": "./dist/types/src/index.d.ts",
  "main": "./dist/cjs/production/index.cjs",
  "module": "./dist/esm/production/index.mjs"
}
